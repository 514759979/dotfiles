pkgname=
pkgver=
pkgrel=1
pkgdesc=''
arch=('any')
url=''
license=('')
source=("")
md5sums=('')

build() {
  cd "${srcdir}/${pkgname}-${pkgver}"

  ./configure --prefix=/usr
  make
}

package() {
  cd "${srcdir}/${pkgname}-${pkgver}"
  make DESTDIR="${pkgdir}" install
}
